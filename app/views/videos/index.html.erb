<p id="notice"><%= notice %></p>

<h1>Listing Videos</h1>

<table =class"table">
  <thead>
    <tr>
      <th>Video url</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= video.video_url %></td>
        <td><%= video.title %></td>
        <td><%= link_to 'Show', video, class:"btn btn-links" %></td>
        <td><%= link_to 'Edit', edit_video_path(video), class:"btn btn-links" %></td>
        <td><%= link_to 'Destroy', video, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-links" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video', new_video_path, class:"btn btn-links" %>
